#include<stdio.h>
main()
{
    int t;
    scanf("%d",&t);
    while(t--)
    {
        long int n,i,s=0;
        scanf("%ld",&n);
        long int a[n];
        for(i=0;i<n;i++)
        {
            scanf("%ld",&a[i]);
        }
        for(i=0;i<n;i++)
        {
            s+=a[i];
        }
        for(i=0;i<n;i++)
        {
            printf("%ld\n",s-a[i]);
        }
    }
}
